CREATE TABLE "WEBSITE_FONTFACE" 
   (	"WEBSITE_ID" NUMBER, 
	"CONTEXT" VARCHAR2(10), 
	"FONT_FAMILY" VARCHAR2(40), 
	"FONT_STYLE" VARCHAR2(20), 
	"FONT_WEIGHT" VARCHAR2(10), 
	"FONT_STRETCH" VARCHAR2(10), 
	"SRC_URL" VARCHAR2(1000), 
	"CREATED_DATE" TIMESTAMP (6) WITH TIME ZONE DEFAULT current_timestamp, 
	"API_URL" VARCHAR2(500), 
	"DEPLOY_FILENAME" VARCHAR2(40)
   ) ;