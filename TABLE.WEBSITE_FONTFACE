CREATE TABLE "WEBSITE_FONTFACE" 
   (	"WEBSITE_ID" NUMBER, 
	"CONTEXT" VARCHAR2(10), 
	"FONT_FAMILY" VARCHAR2(40), 
	"FONT_STYLE" VARCHAR2(6), 
	"FONT_WEIGHT" NUMBER, 
	"FONT_STRETCH" VARCHAR2(4), 
	"SRC_URL" VARCHAR2(1000), 
	"CREATED_DATE" TIMESTAMP (6) WITH TIME ZONE DEFAULT current_timestamp
   ) ;