CREATE TABLE "LOG" 
   (	"ID" NUMBER, 
	"LOG_DATE" TIMESTAMP (6) WITH TIME ZONE DEFAULT current_timestamp, 
	"MESSAGE" CLOB, 
	"PROCEDURE_NAME" VARCHAR2(100), 
	"USER_ID" NUMBER
   ) 
 LOB ("MESSAGE") STORE AS SECUREFILE "LOB_MESSAGE"(ENABLE STORAGE IN ROW 4000 CHUNK 8192
  NOCACHE LOGGING  NOCOMPRESS  KEEP_DUPLICATES ) ;
COMMENT ON TABLE "LOG"  IS 'Utility table capturing application log messages and errors.';