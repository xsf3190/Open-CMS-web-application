CREATE TABLE "LOG" 
   (	"ID" NUMBER GENERATED ALWAYS AS IDENTITY MINVALUE 1 MAXVALUE 9999999999999999999999999999 INCREMENT BY 1 START WITH 1 CACHE 20 NOORDER  NOCYCLE  NOKEEP  NOSCALE  NOT NULL ENABLE, 
	"LOG_DATE" TIMESTAMP (6) WITH TIME ZONE DEFAULT current_timestamp, 
	"MESSAGE" CLOB COLLATE "USING_NLS_COMP", 
	"PROCEDURE_NAME" VARCHAR2(100) COLLATE "USING_NLS_COMP"
   )  DEFAULT COLLATION "USING_NLS_COMP" 
 LOB ("MESSAGE") STORE AS SECUREFILE "LOB_MESSAGE"(ENABLE STORAGE IN ROW CHUNK 8192
  NOCACHE LOGGING  NOCOMPRESS  KEEP_DUPLICATES ) ;