CREATE TABLE "REFRESH_TOKEN" 
   (	"USER_ID" NUMBER, 
	"WEBSITE_ID" NUMBER, 
	"TOKEN_ISSUED" VARCHAR2(64), 
	"CREATED_DATE" TIMESTAMP (6) WITH TIME ZONE DEFAULT current_timestamp, 
	 CONSTRAINT "PK_REFRESH_TOKEN" PRIMARY KEY ("TOKEN_ISSUED")
  USING INDEX  ENABLE
   ) ;

  CREATE UNIQUE INDEX "PK_REFRESH_TOKEN" ON "REFRESH_TOKEN" ("TOKEN_ISSUED") 
  ;