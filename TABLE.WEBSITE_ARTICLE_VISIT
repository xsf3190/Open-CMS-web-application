CREATE TABLE "WEBSITE_ARTICLE_VISIT" 
   (	"WEBSITE_ID" NUMBER NOT NULL ENABLE, 
	"ARTICLE_ID" NUMBER NOT NULL ENABLE, 
	"NAME" VARCHAR2(20), 
	"VALUE" NUMBER, 
	"RATING" VARCHAR2(20), 
	"VISIT_DATE" TIMESTAMP (6) WITH TIME ZONE DEFAULT current_timestamp, 
	"IP_ADDRESS" VARCHAR2(45), 
	"REFERRER" VARCHAR2(200), 
	 CONSTRAINT "FK_WEBSITE_ARTICLE_VISIT" FOREIGN KEY ("WEBSITE_ID", "ARTICLE_ID")
	  REFERENCES "WEBSITE_ARTICLE" ("WEBSITE_ID", "ARTICLE_ID") ON DELETE CASCADE ENABLE
   ) ;