CREATE TABLE "IP_LOOKUP_HASH" 
   (	"IP_ADDRESS_HASH" RAW(32), 
	"CITY" VARCHAR2(30), 
	"COUNTRY_CODE" VARCHAR2(2), 
	"CREATED_DATE" TIMESTAMP (6) WITH TIME ZONE DEFAULT current_timestamp, 
	 CONSTRAINT "PK_IP_LOOKUP_HASH" PRIMARY KEY ("IP_ADDRESS_HASH")
  USING INDEX  ENABLE
   ) ;

  CREATE UNIQUE INDEX "PK_IP_LOOKUP_HASH" ON "IP_LOOKUP_HASH" ("IP_ADDRESS_HASH") 
  ;