CREATE TABLE "WEBSITE_NAVIGATION" 
   (	"ID" NUMBER NOT NULL ENABLE, 
	"WEBSITE_ID" NUMBER NOT NULL ENABLE, 
	"DISPLAY_ORDER" NUMBER NOT NULL ENABLE, 
	"LABEL" VARCHAR2(30), 
	 CONSTRAINT "PK_WEBSITE_NAVIGATION" PRIMARY KEY ("ID")
  USING INDEX  ENABLE, 
	 CONSTRAINT "FK_WEBSITE_NAVIGATION" FOREIGN KEY ("WEBSITE_ID")
	  REFERENCES "WEBSITE" ("ID") ON DELETE CASCADE ENABLE
   ) ;
CREATE UNIQUE INDEX "PK_WEBSITE_NAVIGATION" ON "WEBSITE_NAVIGATION" ("ID") 
  ;