CREATE TABLE "WEBSITE_HIT_ALL_MV" 
   (	"WEBSITE_ID" NUMBER NOT NULL ENABLE, 
	"URL" VARCHAR2(100), 
	"VISIT_DATE" TIMESTAMP (6) WITH TIME ZONE, 
	"LOCATION" VARCHAR2(45), 
	"LCP" NUMBER, 
	"CLS" NUMBER, 
	"INP" NUMBER, 
	"LCP_RATING" VARCHAR2(20), 
	"CLS_RATING" VARCHAR2(20), 
	"INP_RATING" VARCHAR2(20)
   ) ;

  ALTER TABLE "WEBSITE_HIT_ALL_MV" MODIFY ("WEBSITE_ID" NOT NULL ENABLE);