CREATE TABLE "ERR$_PERFORMANCE" 
   (	"ORA_ERR_NUMBER$" NUMBER, 
	"ORA_ERR_MESG$" VARCHAR2(2000) COLLATE "USING_NLS_COMP", 
	"ORA_ERR_ROWID$" UROWID (4000), 
	"ORA_ERR_OPTYP$" VARCHAR2(2) COLLATE "USING_NLS_COMP", 
	"ORA_ERR_TAG$" VARCHAR2(2000) COLLATE "USING_NLS_COMP", 
	"CLD_CLOUD_NAME" VARCHAR2(32767) COLLATE "USING_NLS_COMP", 
	"RESOURCE_TYPE" VARCHAR2(32767) COLLATE "USING_NLS_COMP", 
	"PUBLIC_ID" VARCHAR2(32767) COLLATE "USING_NLS_COMP", 
	"URL" VARCHAR2(32767) COLLATE "USING_NLS_COMP", 
	"TRANSFERSIZE" VARCHAR2(4000) COLLATE "USING_NLS_COMP", 
	"DURATION" VARCHAR2(4000) COLLATE "USING_NLS_COMP", 
	"CONTENTTYPE" VARCHAR2(32767) COLLATE "USING_NLS_COMP", 
	"WINDOW_INNERWIDTH" VARCHAR2(4000) COLLATE "USING_NLS_COMP", 
	"SERVERTIMING" VARCHAR2(32767) COLLATE "USING_NLS_COMP", 
	"USER_ID" VARCHAR2(4000) COLLATE "USING_NLS_COMP", 
	"CAPTURE_DATE" VARCHAR2(4000) COLLATE "USING_NLS_COMP", 
	"USER_AGENT" VARCHAR2(32767) COLLATE "USING_NLS_COMP"
   )  DEFAULT COLLATION "USING_NLS_COMP" ;