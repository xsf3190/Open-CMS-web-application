CREATE TABLE "ERR$_PERFORMANCE" 
   (	"ORA_ERR_NUMBER$" NUMBER, 
	"ORA_ERR_MESG$" VARCHAR2(2000), 
	"ORA_ERR_ROWID$" UROWID (4000), 
	"ORA_ERR_OPTYP$" VARCHAR2(2), 
	"ORA_ERR_TAG$" VARCHAR2(2000), 
	"CLD_CLOUD_NAME" VARCHAR2(32767), 
	"RESOURCE_TYPE" VARCHAR2(32767), 
	"PUBLIC_ID" VARCHAR2(32767), 
	"URL" VARCHAR2(32767), 
	"TRANSFERSIZE" VARCHAR2(4000), 
	"DURATION" VARCHAR2(4000), 
	"CONTENTTYPE" VARCHAR2(32767), 
	"WINDOW_INNERWIDTH" VARCHAR2(4000), 
	"SERVERTIMING" VARCHAR2(32767), 
	"USER_ID" VARCHAR2(4000), 
	"CAPTURE_DATE" VARCHAR2(4000), 
	"USER_AGENT" VARCHAR2(32767)
   ) ;
   COMMENT ON TABLE "ERR$_PERFORMANCE"  IS 'DML Error Logging table for "PERFORMANCE"';