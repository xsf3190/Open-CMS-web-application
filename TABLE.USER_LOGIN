CREATE TABLE "USER_LOGIN" 
   (	"AUTH_PROVIDER" VARCHAR2(10), 
	"EMAIL" VARCHAR2(100), 
	"NAME" VARCHAR2(200), 
	"LOGIN_DATE" TIMESTAMP (6) WITH TIME ZONE DEFAULT current_timestamp, 
	"APEX_SESSION_ID" NUMBER
   ) ;
COMMENT ON TABLE "USER_LOGIN"  IS 'Registers each log in through Social Sign-in (Facebook, Google).';