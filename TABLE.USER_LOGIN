CREATE TABLE "USER_LOGIN" 
   (	"AUTH_PROVIDER" VARCHAR2(10) COLLATE "USING_NLS_COMP", 
	"EMAIL" VARCHAR2(100) COLLATE "USING_NLS_COMP", 
	"NAME" VARCHAR2(200) COLLATE "USING_NLS_COMP", 
	"LOGIN_DATE" TIMESTAMP (6) WITH TIME ZONE DEFAULT current_timestamp
   )  DEFAULT COLLATION "USING_NLS_COMP" ;COMMENT ON TABLE "USER_LOGIN"  IS 'Registers each log in through Social Sign-in (Facebook, Google).';