CREATE TABLE "FONT_STATIC_WGHT" 
   (	"FONT_ID" NUMBER, 
	"WGHT_NORMAL" NUMBER, 
	"WGHT_ITALIC" NUMBER, 
	 CONSTRAINT "FK_FONT_FONT_STATIC_WGHT" FOREIGN KEY ("FONT_ID")
	  REFERENCES "FONT" ("ID") ON DELETE CASCADE ENABLE
   ) ;

  CREATE INDEX "IX_FONT_STATIC_WGHT" ON "FONT_STATIC_WGHT" ("FONT_ID") 
  ;