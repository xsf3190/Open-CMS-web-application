CREATE TABLE "WEBSITE_FILES" 
   (	"WEBSITE_ID" NUMBER, 
	"SHA1" VARCHAR2(40), 
	"DEPLOYED_DATE" TIMESTAMP (6) WITH TIME ZONE DEFAULT current_timestamp, 
	"MD5" VARCHAR2(32), 
	"CONTENT_LENGTH" NUMBER, 
	"PATH" VARCHAR2(250), 
	"SITE_ID" VARCHAR2(40), 
	"MIME_TYPE" VARCHAR2(30), 
	 CONSTRAINT "FK_WEBSITE_WEBSITE_FILES" FOREIGN KEY ("WEBSITE_ID")
	  REFERENCES "WEBSITE" ("ID") ON DELETE CASCADE ENABLE
   ) ;