CREATE TABLE "ORDS_ENDPOINTS" 
   (	"TEMPLATE_ID" NUMBER NOT NULL ENABLE, 
	"URI_TEMPLATE" VARCHAR2(600) NOT NULL ENABLE, 
	"METHOD" VARCHAR2(10) NOT NULL ENABLE, 
	"COMMENTS" VARCHAR2(4000), 
	"PARAMS" VARCHAR2(32767)
   ) ;

  ALTER TABLE "ORDS_ENDPOINTS" MODIFY ("URI_TEMPLATE" NOT NULL ENABLE);

  ALTER TABLE "ORDS_ENDPOINTS" MODIFY ("METHOD" NOT NULL ENABLE);

  ALTER TABLE "ORDS_ENDPOINTS" MODIFY ("TEMPLATE_ID" NOT NULL ENABLE);