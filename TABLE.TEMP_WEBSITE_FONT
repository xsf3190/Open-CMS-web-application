CREATE TABLE "TEMP_WEBSITE_FONT" 
   (	"WEBSITE_ID" NUMBER, 
	"FONT_ID" NUMBER, 
	"CONTEXT" VARCHAR2(10), 
	"WGHT" NUMBER, 
	"WDTH" NUMBER, 
	"OPSZ" NUMBER, 
	"SLNT" NUMBER, 
	"CREATED_DATE" TIMESTAMP (6) WITH TIME ZONE, 
	"UPDATED_DATE" TIMESTAMP (6) WITH TIME ZONE, 
	"FONTFACE" VARCHAR2(1000)
   ) ;