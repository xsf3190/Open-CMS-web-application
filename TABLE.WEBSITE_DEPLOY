CREATE TABLE "WEBSITE_DEPLOY" 
   (	"ID" NUMBER, 
	"WEBSITE_ID" NUMBER, 
	"LOG_TIME" TIMESTAMP (6) WITH TIME ZONE DEFAULT current_timestamp, 
	"STATUS" VARCHAR2(3), 
	"SITE_ID" VARCHAR2(40), 
	"MESSAGE" VARCHAR2(200), 
	 CONSTRAINT "FK_WEBSITE_DEPLOY" FOREIGN KEY ("WEBSITE_ID")
	  REFERENCES "WEBSITE" ("ID") ON DELETE CASCADE ENABLE
   ) ;
COMMENT ON TABLE "WEBSITE_DEPLOY"  IS 'Logs status of website deployment actions.';