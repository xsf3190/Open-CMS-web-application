CREATE TABLE "GOOGLE_FONT_VARIABLE" 
   (	"FAMILY" VARCHAR2(40), 
	"CATEGORY" VARCHAR2(11), 
	"MENU" VARCHAR2(4000), 
	"WGHT_START" NUMBER, 
	"WGHT_END" NUMBER, 
	"WDTH_START" NUMBER, 
	"WDTH_END" NUMBER, 
	"OPSZ_START" NUMBER, 
	"OPSZ_END" NUMBER, 
	"SLNT_START" NUMBER, 
	"SLNT_END" NUMBER, 
	"REGULAR" VARCHAR2(3), 
	"ITALIC" VARCHAR2(3), 
	"LASTMODIFIED" DATE, 
	 CONSTRAINT "PK_GOOGLE_FONT_VARIABLE" PRIMARY KEY ("FAMILY")
  USING INDEX  ENABLE
   ) ;

  CREATE UNIQUE INDEX "PK_GOOGLE_FONT_VARIABLE" ON "GOOGLE_FONT_VARIABLE" ("FAMILY") 
  ;