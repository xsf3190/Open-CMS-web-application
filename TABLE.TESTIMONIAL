CREATE TABLE "TESTIMONIAL" 
   (	"ID" NUMBER GENERATED ALWAYS AS IDENTITY MINVALUE 1 MAXVALUE 9999999999999999999999999999 INCREMENT BY 1 START WITH 1 CACHE 20 NOORDER  NOCYCLE  NOKEEP  NOSCALE  NOT NULL ENABLE, 
	"AUTHOR_USER_ID" NUMBER, 
	"NAME" VARCHAR2(50) COLLATE "USING_NLS_COMP", 
	"POSITION" VARCHAR2(50) COLLATE "USING_NLS_COMP", 
	"DATE_GIVEN" DATE, 
	"TEXT" VARCHAR2(500) COLLATE "USING_NLS_COMP", 
	 CONSTRAINT "PK_testimonial" PRIMARY KEY ("ID")
  USING INDEX  ENABLE
   )  DEFAULT COLLATION "USING_NLS_COMP" ;COMMENT ON TABLE "TESTIMONIAL"  IS 'Customer testimonials are written appraisals of a provided product or service';