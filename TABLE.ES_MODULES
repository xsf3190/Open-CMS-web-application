CREATE TABLE "ES_MODULES" 
   (	"MODULE_NAME" VARCHAR2(50), 
	"SHA1_TEST" VARCHAR2(40) DEFAULT 'not sha1', 
	"SHA1_LIVE" VARCHAR2(40), 
	"UPDATED_SOURCE" TIMESTAMP (6) WITH TIME ZONE, 
	"UPDATED_TEST" TIMESTAMP (6) WITH TIME ZONE, 
	"UPDATED_LIVE" TIMESTAMP (6) WITH TIME ZONE, 
	"DEPLOY_ENVS" NUMBER DEFAULT 1, 
	 CONSTRAINT "PK_ES_MODULES" PRIMARY KEY ("MODULE_NAME")
  USING INDEX  ENABLE
   ) ;

  CREATE UNIQUE INDEX "PK_ES_MODULES" ON "ES_MODULES" ("MODULE_NAME") 
  ;