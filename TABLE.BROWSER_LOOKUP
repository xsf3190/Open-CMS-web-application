CREATE TABLE "BROWSER_LOOKUP" 
   (	"BROWSER" VARCHAR2(60) NOT NULL ENABLE, 
	"CREATED_DATE" TIMESTAMP (6) WITH TIME ZONE DEFAULT current_timestamp, 
	"USER_AGENT_HASH" RAW(32), 
	 CONSTRAINT "PK_BROWSER_LOOKUP" PRIMARY KEY ("USER_AGENT_HASH")
  USING INDEX  ENABLE
   ) ;

  CREATE UNIQUE INDEX "PK_BROWSER_LOOKUP" ON "BROWSER_LOOKUP" ("USER_AGENT_HASH") 
  ;

  ALTER TABLE "BROWSER_LOOKUP" MODIFY ("BROWSER" NOT NULL ENABLE);