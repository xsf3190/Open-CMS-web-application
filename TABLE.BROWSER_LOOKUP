CREATE TABLE "BROWSER_LOOKUP" 
   (	"BROWSER" VARCHAR2(60) NOT NULL ENABLE, 
	"USER_AGENT" VARCHAR2(255), 
	"DATE_CREATED" TIMESTAMP (6) WITH TIME ZONE DEFAULT current_timestamp, 
	 CONSTRAINT "PK_BROWSER_LOOKUP" PRIMARY KEY ("USER_AGENT")
  USING INDEX  ENABLE
   ) ;

  CREATE UNIQUE INDEX "PK_BROWSER_LOOKUP" ON "BROWSER_LOOKUP" ("USER_AGENT") 
  ;

  ALTER TABLE "BROWSER_LOOKUP" MODIFY ("BROWSER" NOT NULL ENABLE);