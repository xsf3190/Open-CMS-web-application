CREATE TABLE "ASSET" 
   (	"ARTICLE_ID" NUMBER, 
	"PUBLIC_ID" VARCHAR2(250), 
	"WIDTH" NUMBER, 
	"HEIGHT" NUMBER, 
	"BYTES" NUMBER, 
	"RESOURCE_TYPE" VARCHAR2(10), 
	"CLD_CLOUD_NAME" VARCHAR2(50), 
	"DISPLAY_ORDER" NUMBER, 
	"CREATED_DATE" TIMESTAMP (6) WITH TIME ZONE DEFAULT current_timestamp, 
	"UPDATED_DATE" TIMESTAMP (6) WITH TIME ZONE, 
	"DESCRIPTION" VARCHAR2(500), 
	"FORMAT" VARCHAR2(5), 
	"ALT_TEXT" VARCHAR2(50), 
	 CONSTRAINT "FK_ARTICLE" FOREIGN KEY ("ARTICLE_ID")
	  REFERENCES "ARTICLE" ("ID") ON DELETE CASCADE ENABLE
   ) ;
  CREATE INDEX "PK_ASSET" ON "ASSET" ("CLD_CLOUD_NAME", "RESOURCE_TYPE", "PUBLIC_ID") 
  ;
ALTER TABLE "ASSET" ADD CONSTRAINT "PK_ASSET" PRIMARY KEY ("CLD_CLOUD_NAME", "RESOURCE_TYPE", "PUBLIC_ID")
  USING INDEX "PK_ASSET"  ENABLE;
COMMENT ON TABLE "ASSET"  IS 'Assets contain the metadata of digital media (photos, videos, audio files) uploaded to the Cloudinary account of the authenticated user.';