  CREATE INDEX "PK_ASSET" ON "ASSET" ("CLD_CLOUD_NAME", "RESOURCE_TYPE", "PUBLIC_ID") 
  ;
ALTER TABLE "ASSET" ADD CONSTRAINT "PK_ASSET" PRIMARY KEY ("CLD_CLOUD_NAME", "RESOURCE_TYPE", "PUBLIC_ID")
  USING INDEX "PK_ASSET"  ENABLE;